{
  "blocks": [
    {
      "type": "section",
      "text": {
        "type": "mrkdwn",
        "text": ":rocket: *New build is available!*"
      }
    },
    <% if @artifacts_url %>
    {
      "type": "section",
      "text": {
        "type": "mrkdwn",
        "text": "*<<%= @artifacts_url %>|Build Artifact>*"
      }
    },
    <% end %>
    {
      "type": "section",
      "text": {
        "type": "mrkdwn",
        "text": "*Version:* <%= @version_number %>\n*Build Number:* <%= @build_number %>\n*Branch:* <%= @branch_name %>"
      },
      "accessory": {
        "type": "image",
        "alt_text": "Tramline"
      }
    }
  ]
}
