<%= render EnhancedTurboFrameComponent.new(dom_id(@app_variant, :edit_variant)) do %>
  <%= render FormComponent.new(model: [@app, @app_variant], url: app_app_variant_path(@app, @app_variant), method: :patch) do |f| %>
    <%= render partial: "form", locals: {form: f, bundle_id: @app.bundle_identifier} %>
    <% f.with_action do %>
      <%= f.F.authz_submit "Update", "archive.svg" %>
    <% end %>
  <% end %>
<% end %>
