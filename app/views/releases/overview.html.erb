<%= render V2::LiveRelease::ContainerComponent.new(@release,
                                                   title: @train.name,
                                                   tab_config: @tab_configuration) do |container| %>
  <% container.with_back_button %>
  <% container.with_tab(title: "Overview", icon: @tab_configuration.dig(:kickoff, :overview, :icon)) do %>
    <%= render V2::LiveRelease::OverviewComponent.new(@release) %>
  <% end %>
<% end %>
