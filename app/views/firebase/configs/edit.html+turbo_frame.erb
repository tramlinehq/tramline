<%= render EnhancedTurboFrameComponent.new("firebase_config") do %>
  <%= render FormComponent.new(model: [@app, @integration], url: app_firebase_config_path(@app), method: :patch) do |f| %>
    <%= render partial: "firebase/configs/form", locals: {f:, integration: @integration, integrable: @app, firebase_ios_apps: @firebase_ios_apps, firebase_android_apps: @firebase_android_apps} %>
    <% f.with_action do %>
      <%= f.F.authz_submit "Update", "plus.svg" %>
    <% end %>
  <% end %>
<% end %>