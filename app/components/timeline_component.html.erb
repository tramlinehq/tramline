<ol class="relative border-s border-gray-200 dark:border-gray-700">
  <% events.each do |event| %>
    <li class="flex items-center gap-x-2 mb-2 ms-4">
      <div class="absolute w-3 h-3 <%= event_color(event) %> rounded-full -start-1.5 border"></div>
      <time class="text-xs font-normal leading-none text-gray-400 dark:text-gray-500"><%= event[:timestamp] %></time>
<!--      <h3 class="text-xs font-medium text-gray-900 dark:text-white"><%#= event[:title] %></h3>-->
      <p class="text-xs font-normal text-gray-500 dark:text-gray-400"><%= event[:description] %></p>
    </li>
  <% end %>
</ol>
